.interaction.left_side
    #panel.interaction.player_panel(ondrop="", ondragover="")
        #video-container
            video#interaction-video.interaction.player_player(mediagroup="preview")
            #crop-lines.crop-lines
                #crop-x1.interaction.fold_x1
                #crop-x2.interaction.fold_x2
                #crop-y1.interaction.fold_y1
                #crop-y2.interaction.fold_y2
        #timeline-back.interaction.player_timeline-back
            #selection-bar-back.interaction.player_selection-bar-back
                #program-selection-bar.interaction.player_program-selection-bar
                #bar-sync1.interaction.player_program-sync-point
                #bar-sync2.interaction.player_program-sync-point
            #timeline-pb.interaction.player_timeline-pb



    //button#interaction_player_cueBlockIn.playerButton_block &larrb;
    //button#interaction_player_setBlockIn.playerButton_block {
    //button#interaction_player_setBlockRemove.playerButton_block &otimes;
    //button#interaction_player_setBlockOut.playerButton_block }
    //button#interaction_player_cueBlockOut.playerButton_block &rarrb;

    button#interaction_player_setProgramIn.playerButton_program [
    button#interaction_player_cueProgramIn.playerButton_program &larrb;
    button#interaction_player_jumpLeft.playerButton_navigate |<
    button#interaction_player_jumpLeft30.playerButton_navigate << -30s
    button#interaction_player_jumpLeft05.playerButton_navigate < -5s

    button#interaction_player_play.playerButton &rtrif;
    button#interaction_player_pause.playerButton &mid;&#8202;&mid;

    button#interaction_player_jumpRight05.playerButton_navigate > +5s
    button#interaction_player_jumpRight30.playerButton_navigate >> +30s
    button#interaction_player_jumpRight.playerButton_navigate >|
    button#interaction_player_cueProgramOut.playerButton_program &rarrb;
    button#interaction_player_setProgramOut.playerButton_program ]
    #current-time.timecode 00:00:00.000

    br
    button#interaction_player_decreaseDelay &LeftTeeArrow; -50ms
    button#interaction_player_addSyncPoint &#9885;
    button#interaction_player_increaseDelay +50ms &map;

    #audio-sync2.sync_point No sync point #2
    #audio-sync1.sync_point No sync point #1
    #audio-delay(style="float: right;width: 80px;text-align: center;padding-top: 8px;font-family: monospace;") 0ms

    br
    .interactions
        button#interaction-refresh Interactions refresh
        table#interaction-table.interaction(frame="border", border="0")
            tr#interaction-table-headers
                th Name
                th guid
                th priority
                th asset
        #interaction-info
            text#interaction-id interaction id

.interaction.right_side
    .setup_container
        table.setup
            tr.setup
                th.setup.d1 Source map
                th.setup.d2 Proposed data
                th.setup.d3 Destination

            tr.setup
                td.setup.d1
                    table#src-map.src
                        tr.src
                            td#src-00.src.row0.col0(rowspan="5") file
                            td#src-00-t00.src.row0.col1 video
                            td#src-00-t00-c00.src.row0.col2 mono
                        tr.src
                            td#src-00-t01.src.row1.col1(rowspan="2") audio
                            td#src-00-t01-c00.src.row1.col2 c00 (FL)
                        tr.src
                            td#src-00-t01-c02.src.row0.col2 c01 (FR)
                        tr.src
                            td#src-00-t02.src.row0.col1(rowspan="2") audio
                            td#src-00-t02-c00.src.row1.col2 c00 (FL)
                        tr.src
                            td#src-00-t02-c02.src.row0.col2 c01 (FR)
                        tr.src
                            td#src-01.src.row1.col0(rowspan="6") file
                            td#src-01-t00.src.row1.col1(rowspan="6") audio
                            td#src-01-t00-c00.src.row1.col2 c00 (FL)
                        tr.src
                            td#src-01-t00-c01.src.row0.col2 c01 (FC)
                        tr.src
