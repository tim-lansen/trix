                                    tr.audio-out.row1
                                        td.audio-out.col0 FC
                                        td.audio-out.col1
                                            select#map-FC
                                                option(value="0") 01-t00-c00
                                                option(value="1") 02-t00-c00
                        tr.propose.row1
                            td#pro-audio-layout-pull.propose.col1.clickable(colspan="2") ==&gt; pull channels ==^
                        tr.propose.row0
