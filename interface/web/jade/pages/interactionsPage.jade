                        tr.src
                            td#src-06.src.row0.col0 file
                            td#src-06-t00.src.row0.col1 audio
                            td#src-06-t00-c00.src.row1.col2 c00 (mono)
                        tr.src
                            td#src-07.src.row1.col0 file
                            td#src-07-t00.src.row1.col1 audio
                            td#src-07-t00-c00.src.row0.col2 c00 (mono)

                td.setup.d2
                    table.propose
                        tr.propose.row0
                            td#update-content-list.propose.col0.clickable Movie
                            td.propose.col1
                                select#pro-movie-select
                                    option(value="1234") Some movie (Some studio)
                                    option(value="332") Movie #2 (Studio)
                        tr.propose.row1
                            td.propose.col0 Video layout
                            td.propose.col1
                                select#pro-video-layout
                                    option(value="mono") Mono
                                    option(value="stereo") Stereo
                        tr.propose.row0
                            td.propose.col0 Video crop
                            td#pro-video-crop.propose.col1 0:0:1920:1080
                        tr.propose.row1
                            td.propose.col0 Audio language
                            td.propose.col1
                                select#pro-audio-lang
                                    option(value="rus") Russian
                                    option(value="eng") English
                                    option(value="blank") Music &amp; Effects
                                    option(value="fra") French
                                    option(value="spa") Spanish
                                    option(value="swe") Swedish
                                    option(value="por") Portuguese
                                    option(value="zho") Chinese
                                    option(value="deu") German
                                    option(value="nor") Norwegian
