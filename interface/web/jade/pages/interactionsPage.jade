                        tr.dst.row1
                            td.dst.col0 ID
                            td#dst-content-id.dst.col1 111
                        tr.dst.row0
                            td.dst.col0 Video
                            td.dst.col1 Video info
                        tr.dst.row1
                            td.dst.col0 Audio
                            td.dst.colX
                                table#audio-out-table.audio-out
                                    tr.audio-out.row0
                                        td.audio-out.col0 rus
                                        td.audio-out.col1 5.1 (1 2 3 4 5 6)
                                    tr.audio-out.row1
